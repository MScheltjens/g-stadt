{
  "name": "g-stadtt",
  "version": "0.0.0",
  "private": true,
  "description": "",
  "author": "",
  "license": "UNLICENSED",
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "test": "turbo run test",
    "test:smoke": "node ./scripts/smoke.js",
    "push": "node ./scripts/push.js",
    "test:e2e": "turbo run test:e2e",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "prettier": "^3.7.4",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.0",
    "turbo": "^2.6.3"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,md}": [
      "prettier --write",
      "pnpm -w exec eslint --fix --cache --cache-location .cache/eslint --"
    ]
  },
  "packageManager": "pnpm@8.15.5",
  "engines": {
    "node": ">=18"
  }
}