{
  "name": "api",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "nest start --watch",
    "build": "nest build",
    "start": "node dist/main.js",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main.js",
    "lint": "eslint . --ext .ts,.js --max-warnings 0",
    "lint:fix": "eslint . --ext .ts,.js --fix --max-warnings 0",
    "db:generate": "prisma generate --schema=./prisma/schema.prisma",
    "db:migrate": "prisma migrate dev --schema=./prisma/schema.prisma",
    "db:studio": "prisma studio --config=prisma/prisma.config.ts",
    "db:reset": "prisma migrate reset --force --schema=./prisma/schema.prisma",
    "db:seed": "ts-node ./prisma/seed.ts"
  },
  "dependencies": {
    "@invicity/constants": "workspace:*",
    "@invicity/contracts": "workspace:*",
    "@nestjs/common": "^11.0.0",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.0",
    "@nestjs/jwt": "^11.0.2",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.0",
    "@nestjs/swagger": "^11.2.4",
    "@prisma/adapter-pg": "7.2.0",
    "@prisma/client": "7.2.0",
    "bcrypt": "^6.0.0",
    "dotenv": "^17.2.3",
    "nestjs-zod": "^5.0.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.17.1",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "json5": "^2.2.3",
    "strip-bom": "^4.0.0",
    "prisma": "7.2.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@swc/cli": "^0.7.10",
    "@swc/core": "^1.15.8",
    "@types/express": "^4.17.17",
    "@types/node": "^22.10.7",
    "@types/pg": "^8.16.0",
    "eslint": "^9.31.0",
    "source-map-support": "^0.5.21"
  }
}